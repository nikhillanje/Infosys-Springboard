def list123(nums): 
    flag=0
    for i in range(2,len(nums)):
        if(nums[i-2]==1 and nums[i-1]==2 and nums[i]==3):
            flag=1
    if(flag==1):
        return True 
    else: 
        return False
    #start writing your code here

    

nums=[1,2,3,4,5]
print(list123(nums))
