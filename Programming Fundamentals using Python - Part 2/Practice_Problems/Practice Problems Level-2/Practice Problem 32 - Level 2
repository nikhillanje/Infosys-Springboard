def check_squares(number): 
    flag=0
    for i in range(0,number):
        for j in range(0,number):
            if((i*i)+(j*j)==number):
                flag=1
                break
    if flag==1:
        return True
    else:
        return False

        
    #start writing your code here


number=68
print(check_squares(number))
